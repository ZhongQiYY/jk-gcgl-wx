<loading hidden="{{loadingHidden}}">
        加载中...
</loading>
<view class="{{search}}">
	<view class="weui-flex" wx:if="{{projectListShow}}">

		<view class="weui-flex__item">
			<picker bindchange="bindBuildChange" value="{{buildIndex}}" range="{{buildArray}}">
				<view class="search-type">
					<image src="/images/sgdw.png"></image>
					<text>{{buildArray[buildIndex]}}</text>
				</view>
			</picker>
		</view>

		<!-- 画竖线 -->
		<view class="middle"></view>

		<view class="weui-flex__item">
			<picker bindchange="bindCategoryChange" value="{{categoryIndex}}" range="{{categoryArray}}">
				<view class="search-type">
					<image src="/images/xmlb.png"></image>
					<text>{{categoryArray[categoryIndex]}}</text>
				</view>
			</picker>
		</view>
	</view>
	<view wx:if="{{projectInfoShow}}">
		<button class="projectBack" bindtap="showProjectList">返回</button>
	</view>
</view>

<view class="page project-list-info" data-weui-theme="{{theme}}">

	<!-- 项目列表页 -->
	<view class="page__bd" wx:if="{{projectListShow}}">

		<view class="weui-cells__title">项目名称</view>
		<view class="weui-cells weui-cells_after-title">

			<view data-text="{{item.projectName}}" data-id="{{item.id}}" data-categoryType="{{item.categoryType}}" bindtap="showProjectInfo" class="weui-cell weui-cell_example " wx:for="{{projectList}}" wx:key="index" hover-class="weui-cell_active">
				<view class="weui-cell__bd">{{item.projectName}}</view>
				<!-- <view class="weui-cell__ft">{{item.buildType}}</view> -->
			</view>
		</view>

	</view>

