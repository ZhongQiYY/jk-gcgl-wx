    <van-field label="监理费用" value="{{projectInfo.pmBuildCost.supervisionCost}}" placeholder="请输入" name="supervisionCost"  bind:change="zjChange"/>
    <van-field label="图审费用" value="{{projectInfo.pmBuildCost.drawCost}}" placeholder="请输入" name="drawCost"  bind:change="zjChange"/>
    <van-field label="其他费用" value="{{projectInfo.pmBuildCost.otherCost}}" placeholder="请输入" name="otherCost"  bind:change="zjChange"/>
    </block>
    <block wx:if="{{categoryType==2}}" wx:key="index">
      <van-field label="工程建安费" value="{{projectInfo.pmParkCost.jiananCost}}" placeholder="请输入" name="jiananCost"  bind:change="zjChange"/>
      <van-field label="园林造价" value="{{projectInfo.pmParkCost.parkCost}}" placeholder="请输入" name="parkCost"   bind:change="zjChange"/>
      <van-field label="配套用房造价" value="{{projectInfo.pmParkCost.supportHouseCost}}" placeholder="请输入" name="supportHouseCost"  bind:change="zjChange"/>
