    <van-field label="图审费用" value="{{projectInfo.pmParkCost.drawCost}}" placeholder="请输入" name="drawCost"  bind:change="zjChange"/>
    <van-field label="其他费用" value="{{projectInfo.pmParkCost.otherCost}}" placeholder="请输入" name="otherCost"  bind:change="zjChange"/>
    </block>
    <block wx:if="{{categoryType==3}}" wx:key="index">
      <van-field label="工程建安费" value="{{projectInfo.pmMunicipalCost.jiananCost}}" placeholder="请输入" name="jiananCost"  bind:change="zjChange"/>
      <van-field label="市政造价" value="{{projectInfo.pmMunicipalCost.municipalCost}}" placeholder="请输入" name="municipalCost"  bind:change="zjChange"/>
      <van-field label="桥梁造价" value="{{projectInfo.pmMunicipalCost.bridgeCost}}" placeholder="请输入" name="bridgeCost"  bind:change="zjChange"/>
