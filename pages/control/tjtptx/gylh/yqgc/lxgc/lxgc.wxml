<view>
	<!--前期 start-->
	<view class="line">
		<picker mode="multiSelector" name="finalTime" value="{{dateTimeArray1[0][dateTime1[0]]}}-{{dateTimeArray1[1][dateTime1[1]]}}-{{dateTimeArray1[2][dateTime1[2]]}} {{dateTimeArray1[3][dateTime1[3]]}}:{{dateTimeArray1[4][dateTime1[4]]}}:{{dateTimeArray1[5][dateTime1[5]]}}" bindchange="changeDateTime1" bindcolumnchange="changeDateTimeColumn1" range="{{dateTimeArray1}}">
			<view class="domes_title">
				零星工程前期时间 : {{dateTimeArray1[0][dateTime1[0]]}}-{{dateTimeArray1[1][dateTime1[1]]}}-{{dateTimeArray1[2][dateTime1[2]]}} {{dateTimeArray1[3][dateTime1[3]]}}:{{dateTimeArray1[4][dateTime1[4]]}}:{{dateTimeArray1[5][dateTime1[5]]}}
			</view>
		</picker>
		<view class='domes_title'>零星工程前期图片</view>
		<view class='ui_cell'>
			<view class='ui_cell_bd'>图片上传</view>
			<view class='ui_cell_ft'>{{hasPicture1}}/9</view>
		</view>
		<view class='ui_uploader_cell'>
			<view class='ui_uploader_item' wx:for="{{files1}}" wx:key="{{index}}">
				<icon class='ui_uploader_item_icon' bindtap='clearImg1' data-index="{{index}}" type="clear" size="20" color="red" />
				<image bindtap='previewImage1' data-index="{{index}}" src='{{item}}'></image>
			</view>
			<view class='ui_uploader' bindtap='chooseImage1' wx:if="{{ files1.length < 10 }}"></view>
		</view>
	</view>
	<view class="divLine">
	</view>

	<!--前期 stop-->

	<!--中期 start-->
	<view class="line">
		<picker mode="multiSelector" name="finalTime" value="{{dateTimeArray2[0][dateTime2[0]]}}-{{dateTimeArray2[1][dateTime2[1]]}}-{{dateTimeArray2[2][dateTime2[2]]}} {{dateTimeArray2[3][dateTime2[3]]}}:{{dateTimeArray2[4][dateTime2[4]]}}:{{dateTimeArray2[5][dateTime2[5]]}}" bindchange="changeDateTime2" bindcolumnchange="changeDateTimeColumn2" range="{{dateTimeArray2}}">
