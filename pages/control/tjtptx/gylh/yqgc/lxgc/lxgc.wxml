				零星工程中期时间 : {{dateTimeArray2[0][dateTime2[0]]}}-{{dateTimeArray2[1][dateTime2[1]]}}-{{dateTimeArray2[2][dateTime2[2]]}} {{dateTimeArray2[3][dateTime2[3]]}}:{{dateTimeArray2[4][dateTime2[4]]}}:{{dateTimeArray2[5][dateTime2[5]]}}
			</view>
		</picker>
		<view class='domes_title'>零星工程中期图片</view>
		<view class='ui_cell'>
			<view class='ui_cell_bd'>图片上传</view>
			<view class='ui_cell_ft'>{{hasPicture2}}/9</view>
		</view>
		<view class='ui_uploader_cell'>
			<view class='ui_uploader_item' wx:for="{{files2}}" wx:key="{{index}}">
				<icon class='ui_uploader_item_icon' bindtap='clearImg2' data-index="{{index}}" type="clear" size="20" color="red" />
				<image bindtap='previewImage2' data-index="{{index}}" src='{{item}}'></image>
			</view>
			<view class='ui_uploader' bindtap='chooseImage2' wx:if="{{ files2.length < 10 }}"></view>
		</view>
	</view>
	<view class="divLine">
	</view>

	<!--中期 stop-->

	<!--后期 start-->
	<view class="line">
		<picker mode="multiSelector" name="finalTime" value="{{dateTimeArray3[0][dateTime3[0]]}}-{{dateTimeArray3[1][dateTime3[1]]}}-{{dateTimeArray3[2][dateTime3[2]]}} {{dateTimeArray3[3][dateTime3[3]]}}:{{dateTimeArray3[4][dateTime3[4]]}}:{{dateTimeArray3[5][dateTime3[5]]}}" bindchange="changeDateTime3" bindcolumnchange="changeDateTimeColumn3" range="{{dateTimeArray3}}">
			<view class="domes_title">
				零星工程后期时间 : {{dateTimeArray3[0][dateTime3[0]]}}-{{dateTimeArray3[1][dateTime3[1]]}}-{{dateTimeArray3[2][dateTime3[2]]}} {{dateTimeArray3[3][dateTime3[3]]}}:{{dateTimeArray3[4][dateTime3[4]]}}:{{dateTimeArray3[5][dateTime3[5]]}}
			</view>
		</picker>
		<view class='domes_title'>零星工程后期图片</view>
		<view class='ui_cell'>
			<view class='ui_cell_bd'>图片上传</view>
			<view class='ui_cell_ft'>{{hasPicture3}}/9</view>
		</view>
		<view class='ui_uploader_cell'>
			<view class='ui_uploader_item' wx:for="{{files3}}" wx:key="{{index}}">
