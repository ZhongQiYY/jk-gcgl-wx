/* pages/charts/charts.wxss */
.table {
  border: 0px solid darkgray;
  padding: 20rpx 10rpx;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  align-items: center;
}
.td {
    border-right: solid #ACC0D8 1px;
    width: 50%;
    height: 140rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}
.bg-w{
  background: #0089dd;
  opacity: 0.6;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  border-right: solid #ACC0D8 1px;
  width: 50%;
  text-align: center;
  justify-content: center;
  background: #0089bb;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}